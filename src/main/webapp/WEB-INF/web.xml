<web-app>
	<display-name>IPSH_Web</display-name>
	<servlet>
		<servlet-name>api-controller</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>api-controller</servlet-name>
